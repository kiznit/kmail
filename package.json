{
  "name": "kmail",
  "version": "0.0.1",
  "description": "Kzinti's Mail",
  "repository": "git://github.com/kiznit/kmail.git",
  "author": "Thierry Tremblay",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 11",
    "not edge < 14",
    "not firefox < 45",
    "not chrome < 49",
    "not safari < 10"
  ],
  "scripts": {
    "start-dev": "yarn clean && yarn build-static && cross-env NODE_ENV=development yarn build-server --env.dev",
    "start": "cross-env NODE_ENV=production node dist/server/server",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn clean && yarn build-server && yarn build-client && yarn build-static",
    "build-client": "webpack --config webpack.client.babel.js",
    "build-server": "webpack --config webpack.server.babel.js",
    "build-static": "cp -R public/ dist/",
    "build-azure": "cross-env AZURE=1 yarn build && cp deployment/azure/web.config .",
    "test": "jest",
    "test-cover": "yarn test --coverage",
    "coverage": "yarn test-cover && opn coverage/lcov-report/index.html"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "applicationinsights": "^1.0.2",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.4",
    "csurf": "^1.9.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "getenv": "^0.7.0",
    "helmet": "^3.11.0",
    "history": "^4.7.2",
    "http-status-codes": "^1.3.0",
    "knex": "^0.14.4",
    "material-ui": "^1.0.0-beta.34",
    "morgan": "^1.9.0",
    "mssql": "^4.1.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-rc.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-pack": "^0.1.5",
    "source-map-support": "^0.5.4",
    "split": "^1.0.1",
    "sqlite3": "^4.0.0",
    "universal-router": "^6.0.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-bluebird": "^1.1.1",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "eventsource-polyfill": "^0.9.6",
    "jest": "^22.4.2",
    "opn-cli": "^3.1.0",
    "sinon": "^4.4.6",
    "sinon-chai": "^3.0.0",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.6.0",
    "why-did-you-update": "^0.1.1"
  }
}
