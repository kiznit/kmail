{
  "name": "kmail",
  "version": "0.0.1",
  "description": "Kzinti's email",
  "repository": "git://github.com/kiznit/kmail",
  "author": "Thierry Tremblay <kiznit@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "start-dev": "yarn clean && yarn build-static && cross-env NODE_ENV=development yarn build-server --mode development",
    "start": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "build": "yarn clean && yarn build-server && yarn build-client && yarn build-static",
    "build-client": "webpack --config webpack.client.babel.js --mode production",
    "build-server": "webpack --config webpack.server.babel.js --mode production",
    "build-static": "mkdirp dist/server && cp -R public/ dist/",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .js,.jsx *.js src",
    "test": "jest",
    "test-cover": "yarn test --coverage",
    "coverage": "yarn test-cover && opn coverage/lcov-report/index.html"
  },
  "keywords": [
    "babel",
    "hmr",
    "isomorphic-rendering",
    "javascript",
    "nodejs",
    "react",
    "redux",
    "ssr",
    "universal-router",
    "webpack"
  ],
